{
  "name": "relay-ecommerce",
  "version": "1.0.0",
  "description": "E-commerce platform built with Relay methodology",
  "main": "index.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc",
    "deploy": "vercel --prod",
    "migrate": "psql $DATABASE_URL -f migrations/001_event_catalog.sql && psql $DATABASE_URL -f migrations/002_initial_events.sql",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "zod": "^3.22.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "@types/js-yaml": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vercel": "^32.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}